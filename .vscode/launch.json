{
	"version": "0.2.0",
	"configurations": [
		
		{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}"
			],
			"outFiles": [
				"${workspaceFolder}/dist/extension.js",
				"${workspaceFolder}/dist/webview.js"
			],
			"preLaunchTask": "npm: compile"
		}
	]
}