{
	"compilerOptions": {
	  "module": "esnext",
	  "target": "es2020",
	  "lib": ["es2020", "dom"],
	  "moduleResolution": "node",
	  "jsx": "react-jsx",
	  "strict": true,
	  "sourceMap": true,
	  "outDir": "out",
	  "rootDir": "src",
	  "esModuleInterop": true,
	  "skipLibCheck": true
	},
	"exclude": ["node_modules", "dist"]
  }